{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":automergeDigest",
    ":automergeLinters",
    ":automergePatch",
    ":automergeRequireAllStatusChecks",
    ":automergeTesters",
    ":automergeTypes",
    ":dependencyDashboard",
    ":docker",
    ":ignoreModulesAndTests",
    ":ignoreUnstable",
    ":label(dependencies)",
    ":prHourlyLimitNone",
    ":prImmediately",
    ":rebaseStalePrs",
    ":semanticCommits",
    ":semanticCommitScope(deps)",
    ":semanticPrefixFixDepsChoreOthers",
    ":separateMultipleMajorReleases",
    ":separateMultipleMinorReleases",
    ":separatePatchReleases",
    ":timezone(Australia/Sydney)",
    ":updateNotScheduled",
    "group:allDigest",
    "group:definitelyTyped",
    "group:githubArtifactActions",
    "group:jsTestNonMajor",
    "group:jsUnitTestNonMajor",
    "group:linters",
    "group:monorepos",
    "group:postcss",
    "group:react",
    "group:rubyOnRails",
    "group:testNonMajor",
    "group:unitTestNonMajor",
    "group:vite",
    "helpers:pinGitHubActionDigests",
    "preview:buildkite",
    "regexManagers:dockerfileVersions",
    "regexManagers:githubActionsVersions",
    "replacements:all",
    "workarounds:all"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true,
    "extends": ["schedule:daily"]
  },
  "vulnerabilityAlerts": {
    "enabled": true,
    "automerge": true,
    "commitMessagePrefix": "fix(security): Security update",
    "addLabels": ["security"]
  },
  "addLabels": [
    "datasource:{{datasource}}",
    "manager:{{manager}}",
    "type:{{updateType}}",
    "versioning:{{versioning}}"
  ],
  "prConcurrentLimit": 1,
  "platform": "github",
  "automergeStrategy": "squash",
  "rangeStrategy": "replace",
  "transitiveRemediation": true,
  "postUpdateOptions": ["npmDedupe", "pnpmDedupe", "yarnDedupeFewer"],
  "schedule": ["* * * * *"],
  "baseBranches": ["main"],
  "minimumReleaseAge": "18 hours",
  "major": {
    "minimumReleaseAge": "7 days"
  }
}
