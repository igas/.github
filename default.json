{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":automergeDigest",
    ":automergeRequireAllStatusChecks",
    ":dependencyDashboard",
    ":label(dependencies)",
    ":prHourlyLimitNone",
    ":prImmediately",
    ":rebaseStalePrs",
    ":timezone(Australia/Sydney)",
    "group:allDigest",
    "helpers:disableTypesNodeMajor",
    "npm:unpublishSafe",
    "replacements:all",
    "workarounds:all"
  ],
  "prConcurrentLimit": 1,
  "platform": "github",
  "automergeStrategy": "squash",
  "schedule": ["* * * * *"],
  "baseBranches": ["main"]
}
