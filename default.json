{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":automergeDigest",
    ":automergeLinters",
    ":automergePatch",
    ":automergeRequireAllStatusChecks",
    ":automergeTypes",
    ":dependencyDashboard",
    ":label(dependencies)",
    ":prHourlyLimitNone",
    ":prImmediately",
    ":rebaseStalePrs",
    ":semanticCommits",
    ":semanticCommitScope(deps)",
    ":semanticPrefixFixDepsChoreOthers",
    ":timezone(Australia/Sydney)",
    "group:allDigest",
    "group:definitelyTyped",
    "group:linters",
    "npm:unpublishSafe",
    "replacements:all",
    "workarounds:all"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true,
    "extends": ["schedule:daily"]
  },
  "prConcurrentLimit": 1,
  "platform": "github",
  "automergeStrategy": "squash",
  "schedule": ["* * * * *"],
  "baseBranches": ["main"]
}
