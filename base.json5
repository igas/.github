{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    ":automergeDigest",
    ":automergeRequireAllStatusChecks",
    "group:allDigest",
    "helpers:disableTypesNodeMajor",
    "npm:unpublishSafe",
    "replacements:all",
    "workarounds:all",
  ],
  platform: "github",
  automergeStrategy: "squash",
  schedule: ["* * * * *"],
  baseBranches: ["main"],
}
